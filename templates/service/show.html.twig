<!DOCTYPE html>

<title>Service</title>

{% block body %}
    <h1>Service</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ service.id }}</td>
            </tr>
            <tr>
                <th>Service_name</th>
                <td>{{ service.serviceName }}</td>
            </tr>
            <tr>
                <th>Create_at</th>
                <td>{{ service.createAt ? service.createAt|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Update_at</th>
                <td>{{ service.updateAt ? service.updateAt|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_service_index') }}">back to list</a>

    <a href="{{ path('admin_service_edit', {'id': service.id}) }}">edit</a>

    {{ include('service/_delete_form.html.twig') }}
{% endblock %}
